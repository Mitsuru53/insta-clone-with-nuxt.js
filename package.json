    "nuxt-firebase": "0.0.3",
    "vue-swal": "^0.1.0",
    "vue2-dropzone": "^3.2.2",
    "vuelidate": "^0.7.4",
    "vuetify": "^1.0.19"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.18.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
