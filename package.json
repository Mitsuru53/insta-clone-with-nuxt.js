{
  "name": "insta-clone",
  "version": "1.0.0",
  "description": "insta clone with Nuxt.js",
  "author": "Mitsuru Hashimoto",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
